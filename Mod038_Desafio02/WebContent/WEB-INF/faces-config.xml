<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
	<navigation-rule>
		<from-view-id>buscar.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>buscado</from-outcome>
			<to-view-id>/buscar.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>ingresar.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ingresado</from-outcome>
			<to-view-id>/buscar.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	

</faces-config>
